{
  "manifest_version": 3,
  "name": "Salesforce Metadata Navigator",
  "version": "1.0.3",
  "description": "Advanced toolkit for Admins: Formula Beautifier, Field Finder, Search Validation rules, Flow Manager and Export List views",
  "permissions": [
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "https://*.salesforce.com/*",
    "https://*.force.com/*",
    "https://*.my.salesforce.com/*",
    "https://*.my.salesforce-setup.com/*",
	"https://*.builder.salesforce-experience.com/*",
	"https://*.cloudforce.com/*",
	"https://*.cloudforce.mil/*",
	"https://*.crmforce.mil/*",
	"https://*.force.com.mcas.ms/*",
	"https://*.force.mil/*",
	"https://*.salesforce.com/*",
	"https://*.salesforce.mil/*",
	"https://*.sfcrmapps.cn/*",
	"https://*.sfcrmproducts.cn/*",
	"https://*.visualforce.com/*",
	"https://*.visualforce.mil/*"
  ],
   "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}